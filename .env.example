# API Key for CAPTCHA solving service (optional)
CAPTCHA_API_KEY=your_2captcha_api_key

# Default download directory
DOWNLOAD_DIR=~/Downloads/youtube

# Default proxy (optional)
DEFAULT_PROXY=http://user:pass@proxy.example.com:8080

# Aria2 settings
ARIA2_PATH=aria2c
ARIA2_MAX_CONNECTIONS=4
ARIA2_SPLIT=4
ARIA2_ENABLED=true

# yt-dlp settings
YTDLP_FORMAT=bestvideo+bestaudio/best
YTDLP_PREFERRED_CODEC=mp4

# Batch settings
BATCH_MAX_CONCURRENT=3